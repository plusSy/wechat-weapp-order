/**index.wxss**/
page{
  background: #eee;
}
.list {
  background: #eee;
}
.ws-address{
  width: 100%;
  color: #3D4245;
  margin-bottom:8px;
  background: #fff;
}
.ws-info{
  width: 100%;
  height:140rpx;
  line-height: 70rpx;
  font-size: 13px;
}
.ws-name{
  float: left;
  padding-left: 3%;
}
.ws-tel{
  float:right;
  padding-right: 3%;
}
.ws-site{
  width: 100%;
  line-height: 50rpx;
  padding:10rpx 0;
  font-size: 12px;
  border-bottom:1px solid #eee;
}
.ws-site view{
  width: 94%;
  margin: 0 auto;
}
.ws-action{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 12px;
}
.ws-default{
  width: 40%;
  margin-left: 3%;
  
}
.weui-agree__text {
  color: #3D4245;
}
.weui-agree__checkbox-icon{
  border: 1px solid #666;
}

.weui-agree__checkbox-icon-check{
  margin-top:-13px;
}
.weui-agree{
  padding:0;
}
.ws-default{
  float: left
}
.ws-edit,.ws-delete{
  float: right;
  margin-right: 30px;
  background-color: #fff;
}
.ws-edit image{
  width:18px;
  height:18px;
  vertical-align: middle;
  margin-right: 5px;
}
.ws-delete image{
  width:18px;
  height:18px;
  vertical-align: middle;
  margin-right: 5px;
}
.cart-tab-wrp{
  height: 90rpx;
  position: fixed;
  bottom: 0rpx;
  background: #f50;
  display: flex;
  flex-direction:row;
  width: 100%;
  z-index: 10;
  color: black
  
}
.cart-tab-item-btn{
    width: 100%;
    display: block;
    height: 90rpx;
    line-height: 90rpx;
    font-size: 28rpx;
    color: #fff;
    font-weight: bolder;
    text-align: center;
}
.prompt {
  text-align: center;
  color:#ccc;
}
.prompt image{
  margin-top:100px;
  width:80rpx;
  height: 80rpx;
}
.prompt text {
  display: block;
  margin-top:10px;
}
.cart-tab-wrp{
  height: 90rpx;
  position: fixed;
  bottom: -2rpx;
  background: #f50;
  display: flex;
  flex-direction:row;
  width: 100%;
  z-index: 10;
  color: black
  
}